{
    "name": "vendi-advertising/yaml-schema-to-sql",
    "type": "library",
    "description": "YAML to MySQL",
    "homepage": "https://github.com/vendi-advertising/yaml-schema-to-sql",
    "license": "MIT",
    "authors": [
        {
            "name": "cjhaas",
            "email": "chris@vendiadvertising.com"
        }
    ],
    "require": {
        "php": "^7",
        "symfony/yaml": "^4.0",
        "pear/console_table": "^1.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.0",
        "sensiolabs/security-checker": "^4.1",
        "jakub-onderka/PHP-Parallel-Lint": "dev-master",
        "friendsofphp/php-cs-fixer": "^2.2 || ^2.7"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {"Vendi\\YamlSchemaToSql\\": "src/"}
    }
}
