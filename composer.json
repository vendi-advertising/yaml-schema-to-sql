{
    "name": "vendi-advertising/yaml-schema-to-sql",
    "type": "library",
    "description": "YAML to MySQL",
    "homepage": "https://github.com/vendi-advertising/yaml-schema-to-sql",
    "license": "MIT",
    "authors": [
        {
            "name": "cjhaas",
            "email": "chris@vendiadvertising.com"
        }
    ],
    "require": {
        "php": "^7",
        "symfony/yaml": "4.0.3",
        "webmozart/path-util": "2.3.0"
    },
    "require-dev": {
        "symfony/var-dumper": "4.0.3",
        "sensiolabs/security-checker": "4.1.7",
        "jakub-onderka/PHP-Parallel-Lint": "dev-master",
        "friendsofphp/php-cs-fixer": "2.10.0"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {"Vendi\\YamlSchemaToSql\\": "src/"}
    }
}
